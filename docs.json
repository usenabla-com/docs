{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Nabla Docs",
  "colors": {
    "primary": "#FF5F1F",
    "light": "#FF5C00",
    "dark": "#CC5500"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": [
              "index",
              "quickstart"
            ]
          },
          {
            "group": "Usage Examples",
            "pages": [
              "examples/fedramp-assessment",
              "examples/cmmc-assessment",
              "examples/diagram-generation",
              "examples/fips-assessment"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "openapi": "api-reference/openapi.json",
        "groups": [
          {
            "group": "API documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Control Mapping Schemas",
            "pages": [
              "api-reference/schemas/800-53",
              "api-reference/schemas/800-171",
              "api-reference/schemas/800-172",
              "api-reference/schemas/fips-140-3"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Homepage",
          "href": "https://www.usenabla.com",
          "icon": "house"
        },
        {
          "anchor": "Get an API Key",
          "href": "https://cal.com/team/nabla/nabla-pilot-interest-call",
          "icon": "key"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@usenabla.com"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Get an API Key",
      "href": "https://cal.com/team/nabla/nabla-pilot-interest-call"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/mintlify",
      "github": "https://github.com/mintlify",
      "linkedin": "https://linkedin.com/company/mintlify"
    }
